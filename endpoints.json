{
    "description": "Marcelão está adorando a nova automação de seu colega de trabalho/neto gabriel",
    "sequences": [
        "EnvironmentVariables",
        "SetHeaders",
        "ErrorHandler",
        "CadastraItem"
    ],
    "endpoints": [
        {
            "name": "jsonPlaceHolder",
            "http": "http://jsonplaceholder.typicode.com/todos/{id}"
        }
    ],
    "apis": [
        {
            "context": "/users",
            "resources": [
                {"get": "/"},
                {"get": "/getName"},
                {"post": "/changeName"}
            ]
        },
        {
            "context": "/cadastraItens",
            "resources": [
                {"get": "/{item}"},
                {"post": "/{item}"}
            ]
        }
    ],
    "messageProcessorEStore": [
        {
            "processor": "messageProcessorTeste",
            "store": "messageStoreTeste",
            "queue": "messageQueueTeste",
            "sequence": "exeQueue"
        }
    ],
    "templates": [
        "SVC-SalesForceBulkAPIV2JobSuccessfulTemplate"
    ],
    "resources": [
        "TransformJsonObject.xslt",
        "schemaValidation.json"
    ],
    "dataServices": {
        "function": "quote_upsert",
        "properties": [
            "id",
            "name",
            "other"
        ]
    },
    "env": [
        {"Ambiente": ["HOMOLOG", "PRODUCAO"]},
        {"TZ": ["America/saoPaulo", "America/saoPaulo"]},
        {"CADASTRA_ITEM_DB_URL": ["sdbc://mysql:05", "sdbc://mysql:20"]},
        {"CADASTRA_ITEM_DB_USER": ["admin", "admin"]},
        {"CADASTRA_ITEM_DB_PASSWORD": ["1234", "1234"]},
        {"PROGRESS_API_URL": ["http://progress-homolog.com", "http://progress-producao.com"]},
        {"ZABBIX_API_URL": ["http://zabbix-homolog.com", "http://zabbix-prdoucao.com"]},
        {"GRAFANA_URL": ["http://grafana-homolog.com", "http://grafana-producao.com"]}
    ]
}