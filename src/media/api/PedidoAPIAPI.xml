<?xml version="1.0" encoding="UTF-8"?>
<api context="/pedido" name="HealthCheckAPI" xmlns="http://ws.apache.org/ns/synapse">
    <resource methods="POST"    uri-template="/v1/pedido">
    <inSequence>
        <log level="custom">
            <property name="Requisição recebido" value="/v1/pedido"/>
        </log>
        <property name="HTTP_SC" scope="axis2" type="STRING" value="200"/>
        <respond/>
    </inSequence>
    <outSequence/>
    <faultSequence/>
</resource>           
<resource methods="PUT"    uri-template="/v1/notafiscal-agenciamento">
    <inSequence>
        <log level="custom">
            <property name="Requisição recebido" value="/v1/notafiscal-agenciamento"/>
        </log>
        <property name="HTTP_SC" scope="axis2" type="STRING" value="200"/>
        <respond/>
    </inSequence>
    <outSequence/>
    <faultSequence/>
</resource>           

</api>
