<?xml version="1.0" encoding="UTF-8"?>
<api context="/conta/v1" name="HealthCheckAPI" xmlns="http://ws.apache.org/ns/synapse">
    <resource methods="POST"    uri-template="/chatbot/clienteFinal">
    <inSequence>
        <log level="custom">
            <property name="Requisição recebido" value="/chatbot/clienteFinal"/>
        </log>
        <property name="HTTP_SC" scope="axis2" type="STRING" value="200"/>
        <respond/>
    </inSequence>
    <outSequence/>
    <faultSequence/>
</resource>           
<resource methods="GET"    uri-template="/{cnpj}">
    <inSequence>
        <log level="custom">
            <property name="Requisição recebido" value="/{cnpj}"/>
        </log>
        <property name="HTTP_SC" scope="axis2" type="STRING" value="200"/>
        <respond/>
    </inSequence>
    <outSequence/>
    <faultSequence/>
</resource>           

</api>
