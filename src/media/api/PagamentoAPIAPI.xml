<?xml version="1.0" encoding="UTF-8"?>
<api context="/pagamento" name="HealthCheckAPI" xmlns="http://ws.apache.org/ns/synapse">
    <resource methods="POST"    uri-template="/v1/boleto">
    <inSequence>
        <log level="custom">
            <property name="Requisição recebido" value="/v1/boleto"/>
        </log>
        <property name="HTTP_SC" scope="axis2" type="STRING" value="200"/>
        <respond/>
    </inSequence>
    <outSequence/>
    <faultSequence/>
</resource>           
<resource methods="POST"    uri-template="/v1/eventoGalaxPay">
    <inSequence>
        <log level="custom">
            <property name="Requisição recebido" value="/v1/eventoGalaxPay"/>
        </log>
        <property name="HTTP_SC" scope="axis2" type="STRING" value="200"/>
        <respond/>
    </inSequence>
    <outSequence/>
    <faultSequence/>
</resource>           

</api>
